 <script>
document.getElementById("confEventSelect").addEventListener("change", function () {
    console.log('v', this.value);
    //  show data-id_conf_event = this.value remove hidden class
	//  hide data-id_conf_event != this.value add hidden class
 
	var elements = document.querySelectorAll('[data-id_conf_event]');

	console.log(elements, 'a');

	for (var i = 0; i < elements.length; i++) {
		if (elements[i].getAttribute('data-id_conf_event') == this.value) {
			elements[i].classList.remove('hidden');
		} else {
			elements[i].classList.add('hidden');
		}
	}
});



 </script>
 

